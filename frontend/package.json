{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "types": "tsc --noEmit --project ./tsconfig.node.json && tsc --noEmit --project ./tsconfig.json",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/.eslintcache --ext .ts,.tsx . --fix",
    "format": "prettier --log-level warn --cache --write {src/**/*,*}.{ts,tsx,js,json,yaml,scss,css} !**/{node_modules,dist}/**/*",
    "stylecheck": "sass --update -q ./src:./node_modules/.cache/.stylecheck_cache",
    "stylelint": "stylelint --cache --cache-location ./node_modules/.cache/.stylelint_cache src/**/*.scss --fix"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.90.6",
    "formik": "^2.4.6",
    "formik-validator-zod": "^2.2.0",
    "js-cookie": "^3.0.5",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.4",
    "reset-css": "^5.0.2",
    "sass": "^1.93.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^24.9.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitejs/plugin-react": "^5.1.0",
    "eslint": "^9.38.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-unused-imports": "^4.3.0",
    "globals": "^16.4.0",
    "prettier": "^3.6.2",
    "stylelint": "^16.25.0",
    "stylelint-config-prettier-scss": "^1.0.0",
    "stylelint-config-standard-scss": "^16.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^7.1.12"
  }
}
